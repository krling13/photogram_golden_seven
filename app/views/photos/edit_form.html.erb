<h1>Edit Photo</h1>
<form action="http://localhost:3000/photos/<%= @thephoto.id %>/edit_photo">
<div>
  <label>Caption</label>
  <input type="text" name="the_caption" value="<%= @thephoto.caption %>">
</div>
<div>
  <label>Source</label>
  <input type="text" name="the_source" value="<%= @thephoto.source %>">
</div>
<div>
<button>Edit Photo</button>
</div>
</form>




